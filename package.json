{
  "name": "website",
  "version": "1.0.0",
  "description": "Meowdding Website!",
  "scripts": {
    "dev": "pnpm exec live-server",
    "prettier": "pnpm exec prettier --write .",
    "prettier:check": "pnpm exec prettier --check .",
    "tailwind": "pnpm exec tailwindcss -i input.css -o output.css",
    "tailwind:watch": "pnpm exec tailwindcss -i input.css -o output.css --watch"
  },
  "keywords": [],
  "author": "Kathund",
  "engines": {
    "node": ">=20.16.0",
    "pnpm": ">=9.9.0",
