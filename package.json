    "yarn": "forbidden, use pnpm instead",
    "bun": "forbidden, use pnpm instead"
  },
  "packageManager": "pnpm@9.9.0",
  "devDependencies": {
    "live-server": "^1.2.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "tailwindcss": "^4.1.13"
  }
}
